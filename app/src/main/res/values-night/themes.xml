<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.InTernak" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/primary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="bottomNavigationStyle">@style/Widget.BottomNavigationView</item>
        <item name="colorControlHighlight">@android:color/transparent</item>
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Widget.BottomNavigationView" parent="Widget.Design.BottomNavigationView">
        <item name="itemBackground">@android:color/transparent</item>
        <item name="itemRippleColor">@android:color/transparent</item>
    </style>

</resources>